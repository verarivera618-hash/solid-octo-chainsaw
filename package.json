{
  "name": "solid-octo-chainsaw",
  "version": "1.0.0",
  "description": "Advanced backtesting framework for trading strategy prediction and analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test:alpaca": "tsc && node dist/examples/alpaca-test-connection.js",
    "live:alpaca": "tsc && node dist/examples/alpaca-live-trading.js"
  },
  "keywords": [
    "backtesting",
    "trading",
    "finance",
    "strategy",
    "analysis",
    "prediction",
    "alpaca",
    "live-trading"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.6.1",
    "lodash": "^4.17.21"
  }
}
